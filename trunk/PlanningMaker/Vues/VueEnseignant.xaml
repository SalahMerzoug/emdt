<UserControl x:Class="PlanningMaker.Vues.VueEnseignant"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
     xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" mc:Ignorable="d">

    <StackPanel x:Name="LayoutRoot">
        <StackPanel Orientation="Horizontal">
            <Label Content="Nom" Width="70"/>
            <TextBox Name="nom" Text="{Binding Path=Nom}" Width="100"/>
        </StackPanel>
        <StackPanel Orientation="Horizontal">
            <Label Content="Prénom" Width="70"/>
            <TextBox Name="prenom" Text="{Binding Path=Prenom}" Width="100"/>
        </StackPanel>
    </StackPanel>
</UserControl>
